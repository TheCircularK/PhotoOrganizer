{
  "id": "com.thecirculark.photoorganizer",
  "runtime": "org.gnome.Platform",
  "runtime-version": "49",
  "sdk": "org.gnome.Sdk",
  "command": "photoorganizer",
  "finish-args": [
    "--share=network",
    "--share=ipc",
    "--socket=fallback-x11",
    "--device=dri",
    "--socket=wayland"
  ],
  "cleanup": [
    "/include",
    "/lib/pkgconfig",
    "/man",
    "/share/doc",
    "/share/gtk-doc",
    "/share/man",
    "/share/pkgconfig",
    "*.la",
    "*.a"
  ],
  "modules": [
    {
      "name": "photoorganizer",
      "builddir": true,
      "buildsystem": "meson",
      "sources": [
        {
          "type": "git",
          "url": "https://github.com/TheCircularK/PhotoOrganizer",
          "branch": "main"
        }
      ]
    },
    {
      "name": "python3-exif",
      "buildsystem": "simple",
      "build-commands": ["python3 -m pip install --no-index --no-build-isolation --prefix=/app ."],
      "sources": [
        {
          "type": "archive",
          "url": "https://files.pythonhosted.org/packages/52/21/34c46fbda54648afdcc66f0eec0ac91675361017abcdcdd23bd88fa74cc5/exif-1.6.1.tar.gz",
          "sha256": "763599b89b9b67495713060a703f32d1874abf8f0628c9d77711c2c06a5f44c8"
        }
      ],
      "build-options": {}
    }
  ]
}
