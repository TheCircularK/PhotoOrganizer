{
  "id": "com.thecirculark.photoorganizer",
  "runtime": "org.gnome.Platform",
  "sdk": "org.gnome.Sdk",
  "command": "photoorganizer",
  "finish-args": [
    "--share=network",
    "--share=ipc",
    "--socket=fallback-x11",
    "--device=dri",
    "--socket=wayland"
  ],
  "cleanup": [
    "/include",
    "/lib/pkgconfig",
    "/man",
    "/share/doc",
    "/share/gtk-doc",
    "/share/man",
    "/share/pkgconfig",
    "*.la",
    "*.a"
  ],
  "modules": [
    {
      "name": "photoorganizer",
      "builddir": true,
      "buildsystem": "meson",
      "sources": [
        {
          "type": "git",
          "url": "file:///home/andrew/Projects/PhotoOrganizer",
          "branch": "main"
        }
      ]
    },
    {
      "name": "python3-exif",
      "buildsystem": "simple",
      "build-commands": ["pip3 install --prefix=/app exif"],
      "sources": []
    }
  ]
}
